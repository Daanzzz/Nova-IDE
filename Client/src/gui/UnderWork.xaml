<UserControl x:Class="Client.UnderWork"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Client"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="177*"/>
            <ColumnDefinition Width="223*"/>
        </Grid.ColumnDefinitions>
        <Image Source="/img/underWork.png" HorizontalAlignment="Left" Margin="58,23,0,135" RenderTransformOrigin="0.492,0.385" Width="684" Grid.ColumnSpan="2" />
        <Image Source="/img/button.png" HorizontalAlignment="Left" Margin="264,187,0,64" RenderTransformOrigin="0.492,0.385" Width="272" Grid.ColumnSpan="2" />

        <TextBlock Text="Go Back" HorizontalAlignment="Left"  VerticalAlignment="Top"  FontSize="30"  FontWeight="Bold" Opacity="0.9" Foreground="#b4abc9" Margin="342,249,0,0" Height="42" Width="116" Grid.ColumnSpan="2" />

        <Button BorderThickness="0" Background="Transparent" Margin="269,254,269,131" Opacity="0" Click="Go_Back" Cursor="Hand" Grid.ColumnSpan="2"/>
    </Grid>
</UserControl>
